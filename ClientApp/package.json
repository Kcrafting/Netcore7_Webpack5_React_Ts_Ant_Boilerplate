{
  "name": "react-typescript-webpack-starter",
  "version": "1.0.0",
  "license": "ISC",
  "description": "A starter template for React with Typescript using Webpack 5",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "debugstart": "node --inspect-brk=5859 ./node_modules/webpack/bin/webpack --config webpack/webpack.config.js --env env=dev",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Webpack 5",
    "Starter Template",
    "Boilerplate"
  ],
  "author": "Vishwas",
  "dependencies": {
    "caniuse-lite": "^1.0.30001439",
    "connected-react-router": "^6.9.3",
    "dayjs": "^1.11.7",
    "fs": "^0.0.1-security",
    "history": "^5.3.0",
    "http-proxy-middleware": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.5",
    "react-scripts": "^5.0.1",
    "react-virtualized-auto-sizer": "^1.0.7",
    "redux": "^4.2.0",
    "redux-first-history": "^5.1.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.0",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/runtime": "^7.13.10",
    "@minko-fe/use-antd-resizable-header": "^2.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-resizable": "^3.0.3",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "ahooks": "^3.7.2",
    "antd": "5.0.7",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.1.0",
    "husky": "4",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rc-resize-observer": "^1.2.0",
    "rc-tween-one": "^3.0.6",
    "react-data-grid": "^7.0.0-beta.20",
    "react-refresh": "^0.9.0",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^6.4.5",
    "react-window": "^1.8.8",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
